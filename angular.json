{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-sorter": {
      "projectType": "library",
      "root": "projects/ng-sorter",
      "sourceRoot": "projects/ng-sorter/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-sorter/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-sorter/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-sorter/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ng-sorter/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "de5e7a7d-fb11-4846-a2b8-5f95c44f4194"
  }
}
